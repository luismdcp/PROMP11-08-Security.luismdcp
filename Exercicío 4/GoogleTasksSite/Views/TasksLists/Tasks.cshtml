@model IEnumerable<Task>

@{
    ViewBag.Title = "Tasks";
}

<h2>Tasks</h2>

<table>
    <tr>
        <th>Kind</th>
        <th>Etag</th>
        <th>Title</th>
        <th>Updated</th>
        <th>SelfLink</th>
        <th>Parent</th>
        <th>Position</th>
        <th>Notes</th>
        <th>Due</th>
        <th>Completed</th>
        <th>Deleted</th>
        <th>Hidden</th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>@Html.DisplayFor(modelItem => item.Kind)</td>
        <td>@Html.DisplayFor(modelItem => item.Etag)</td>
        <td>@Html.DisplayFor(modelItem => item.Title)</td>
        <td>@Html.DisplayFor(modelItem => item.Updated)</td>
        <td>@Html.DisplayFor(modelItem => item.SelfLink)</td>
        <td>@Html.DisplayFor(modelItem => item.Parent)</td>
        <td>@Html.DisplayFor(modelItem => item.Position)</td>
        <td>@Html.DisplayFor(modelItem => item.Notes)</td>
        <td>@Html.DisplayFor(modelItem => item.Due)</td>
        <td>@Html.DisplayFor(modelItem => item.Completed)</td>
        <td>@Html.DisplayFor(modelItem => item.Deleted)</td>
        <td>@Html.DisplayFor(modelItem => item.Hidden)</td>
        <td>@Html.ActionLink("Back", "TaskLists")</td>
    </tr>
}

</table>